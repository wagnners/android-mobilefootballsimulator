        </activity>
        <activity android:name=".Time" />
        <activity android:name=".EscolherClube" />
        <activity android:name=".Loja" />
        <activity android:name=".Campeonato" />
        <activity android:name=".JogadorActivity" />
        <activity android:name=".Estadio" />
        <activity android:name=".JogosActivity" />
        <activity android:name=".ResultadosActivity"></activity>
    </application>

</manifest>